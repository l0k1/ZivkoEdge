  <animation>
    <type>rotate</type>
    <object-name>TrimSwitch.Lever</object-name>
    <property>controls/flight/elevator-trim-switch</property>
    <factor>30</factor>
    <center>
      <x-m>0</x-m>
      <y-m>-0.290482</y-m>
      <z-m>0.0202506</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>Label.NoseDown</object-name>
    <visible>true</visible>
    <action>
      <name>Trim Down</name>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>nasal</command>
        <script>controls.elevatorTrim(1)</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property>controls/flight/elevator-trim-switch</property>
        <value>-1</value>
      </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <property>controls/flight/elevator-trim-switch</property>
          <value>0</value>
        </binding>
      </mod-up>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>Label.NoseUp</object-name>
    <visible>true</visible>
    <action>
      <name>Trim Up</name>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>nasal</command>
        <script>controls.elevatorTrim(-1)</script>
      </binding>
      <binding>
        <command>property-assign</command>
        <property>controls/flight/elevator-trim-switch</property>
        <value>1</value>
      </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <property>controls/flight/elevator-trim-switch</property>
          <value>0</value>
        </binding>
      </mod-up>
    </action>
  </animation>

